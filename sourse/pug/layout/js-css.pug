- const version = '0.0.1'
mixin allcss() 
	link(href="libs/swiper/swiper.min.css", rel="stylesheet")
	link(href="libs/@fancyapps/ui/fancybox.css", rel="stylesheet")
	link(rel="preload" href="fonts/MADEEvolveSans.woff2" as="font" type="font/woff2" crossorigin)
	link(rel="preload" href="fonts/MADEEvolveSansBold.woff2" as="font" type="font/woff2" crossorigin)
	link(rel="preload" href="fonts/MADEEvolveSansLight.woff2" as="font" type="font/woff2" crossorigin)
	link(rel="preload" href="fonts/MADEEvolveSansMedium.woff2" as="font" type="font/woff2" crossorigin)
	link(href=`css/main.min.css?v=${version}`, rel="stylesheet")

mixin allcjs() 
	script(src="libs/jquery/jquery.min.js")
	script(src="libs/@fancyapps/ui/fancybox.umd.js") 
	script(src="libs/swiper/swiper-bundle.min.js")  
	script(src="libs/inputmask/inputmask.min.js") 
	script(src="libs/masonry-layout/masonry.pkgd.min.js") 
	script(src=`js/common.js?v=${version}`)